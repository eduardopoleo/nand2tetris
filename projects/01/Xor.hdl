CHIP Xor {
  /* This just mean that a, b are going to dertemine the name of the
     inputs for each gate
   */
  IN a, b;
  /* Simirarly this is the name of the output for each input */
  OUT out;

  PARTS:
    Or(a=a, b=b, out=w1);
    Nand(a=a, b=b, out=w2);
    And(a=w1, b=w2, out=out);
}
