CHIP Xor {
  /* This just mean that a, b are going to dertemine the name of the
     inputs for each gate
   */
  IN a, b;
  /* Simirarly this is the name of the output for each input */
  OUT out;

  PARTS:
  /* I guess the first inputs required the particle in */
  Not (in=a, out=nota);
  Not (in=b, out=notb);
  And (a=a, b=notb, out=aAndNotb);
  And (a=nota, b=b, out=notaAndb);
  Or  (a=aAndNotb, b=notaAndb, out=out);
}
